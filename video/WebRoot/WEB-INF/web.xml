<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>video</display-name>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.video.server.LoginServer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>fx</servlet-name>
		<servlet-class>com.video.server.FXServer</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet-mapping>
		<servlet-name>fx</servlet-name>
		<url-pattern>/fx.do</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>show_type</servlet-name>
		<servlet-class>com.video.server.ShowTypeServer</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>show_type</servlet-name>
		<url-pattern>/show_type.do</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>addType</servlet-name>
		<servlet-class>com.video.server.AddTypeServer</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>addType</servlet-name>
		<url-pattern>/addType.do</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>showDireList</servlet-name>
		<servlet-class>com.video.server.ShowDireListServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showDireList</servlet-name>
		<url-pattern>/showDireList.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>showCreateKC</servlet-name>
		<servlet-class>com.video.server.ShowCreateKCServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showCreateKC</servlet-name>
		<url-pattern>/showCreateKC.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>findType</servlet-name>
		<servlet-class>com.video.server.FindTypeServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findType</servlet-name>
		<url-pattern>/findType.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ajax</servlet-name>
		<servlet-class>com.video.server.AjaxServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ajax</servlet-name>
		<url-pattern>/ajax.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>up</servlet-name>
		<servlet-class>com.video.server.Upload</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>up</servlet-name>
		<url-pattern>/up.do</url-pattern>
	</servlet-mapping>
</web-app>